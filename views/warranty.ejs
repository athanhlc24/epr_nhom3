<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>BaoHanh</title>
    <link rel="stylesheet" href="/css/service.css"/>

</head>
<%- include("css.ejs") %>


<body>
<%- include("header.ejs") %>
<%- include("nav.ejs")%>
<section class="section main">
    <div class="container">
        <div class="row">
            <article class="content col-md-12">
                <div class="col-md-3">
                    <h5>Service & Workshop</h5>
                </div>
                <div class="col-md-9">
                    <div class="dropdown service">
                        <a href="#" id="dLabel" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Warranties
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="dLabel">
                            <%for(var i =0;i<brandList.length;i++){%>
                            <li><a href="/warranty?BrName=<%=brandList[i].BrName%>"><%=brandList[i].BrName%></a></li>
                            <%}%>
                        </ul>
                    </div>
                    <div class="dropdown service">
                        <a href="#" id="dLabel1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Phu Tung
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="dLabel1">
                            <%for(var i =0;i<brandList.length;i++){%>
                            <li><a href="/phutung?BrName=<%=brandList[i].BrName%>"><%=brandList[i].BrName%></a></li>
                            <%}%>
                        </ul>
                    </div>
                    <div class="service"><a href="#" >Sua Chua & Cham Soc</a></div>
                </div>
                <div class="col-md-12 warranty-content">
                    <div class="col-md-1"></div>
                    <div class="col-md-8">
                        <%for(var i =0;i<brandWarrantyList.length;i++){%>

                        <p><%=brandWarrantyList[i].BrWarranty%></p>

                        <%}%>
                    </div>

                </div>
            </article>
        </div>
    </div>
</section>
<%- include("footer.ejs")%>
<script type="text/javascript" src="/jQuery/jquery3.6.0.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@3.4.1/dist/js/bootstrap.min.js" integrity="sha384-aJ21OjlMXNL5UyIl/XNwTMqvzeRMZH2w8c5cRVpzpU8Y5bApTppSuUkhZXN0VxHd" crossorigin="anonymous"></script></script>


</body>
</html>