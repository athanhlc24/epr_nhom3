<link rel="stylesheet" href="/css/car-discount.css">
<body>
<div class="container">
    <div class="row">
        <h3 id="car-discount"><b>CAR DISCOUNT</b></h3>
        <div class="title-border2">
            <div class="title-border-inner2"></div>
            <div class="title-border-inner2"></div>
            <div class="title-border-inner2"></div>
            <div class="title-border-inner2"></div>
        </div>

        <%for (var i=0;i<bestList.length;i++){%>
        <%if(bestList[i].HotCars == 0){%>
        <div class="list-selling-car col-md-4">
            <a href="#">
                <div class="thumb">
                    <img src="/demo-list/demo-list.png"/>
                </div>
            </a>
            <h4> <%=bestList[i].Name%> </h4>
            <p><i class="fa-solid fa-gas-pump"></i> <%=bestList[i].Fueltype%></p>
            <p><i class="fa-solid fa-calendar-days"></i> <%=bestList[i].Year%></p>
            <p><i class="fa-solid fa-dollar-sign"></i> <%=bestList[i].Price *90/100; %> </p>

        </div>
        <%}%>
        <%}%>
    </div>
</div>
</body>
